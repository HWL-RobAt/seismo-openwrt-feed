CONFIG_BRN_CHANNEL=1               # WiFi channel (will be used twice:
                                   #                 "iwconfig ath0 channel $channel" and
                                   #                 inside click config for beacon frames)
CONFIG_BRN_ESSID=BerlinRoofNet     # ESSID used by click

# CONFIG_GATEWAY_MAC=00:11:22:33:44:55


CLICK_BIN_PATH=/usr/bin            # location of click-binary
                                   # (usually /usr/bin if installed from ipkg)

CLICK_CONF_PATH=/etc/berlinroofnet # location of click-include and config files
